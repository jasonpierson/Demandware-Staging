          </transition>
        </branch>
      </node>
      <transition target-connector="in1" target-path="./+1">
        <transition-display>
          <bend-point relative-to="source" x="0" y="2"/>
        </transition-display>
      </transition>
    </segment>
    <segment>
      <node>
        <join-node/>
        <node-display x="-1" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="GetNearestStores" pipelet-set-identifier="bc_api">
          <config-property key="DistanceUnit" value="mi"/>
          <key-binding alias="ZipLocation" key="PostalCode"/>
          <key-binding alias="&quot;US&quot;" key="CountryCode"/>
          <key-binding alias="Stores" key="Stores"/>
          <key-binding alias="City" key="City"/>
          <key-binding alias="State" key="State"/>
          <key-binding alias="50" key="MaxDistance"/>
          <key-binding alias="&quot;mi&quot;" key="DistanceUnit"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <decision-node condition-key="Stores.size()!=0" condition-operator="expr"/>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="yes">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="source" x="0" y="1"/>
            </transition-display>
